ib-type="author"><name><surname>Galasko</surname><given-names>Douglas</given-names></name><xref ref-type="aff" rid="A19">s</xref></contrib><contrib contrib-type="author"><name><surname>Graff-Radford</surname><given-names>Jonathan</given-names></name><xref ref-type="aff" rid="A12">l</xref></contrib><contrib contrib-type="author"><name><surname>Graff-Radford</surname><given-names>Neill R.</given-names></name><xref ref-type="aff" rid="A20">t</xref></contrib><contrib contrib-type="author"><name><surname>Hof</surname><given-names>Patrick R.</given-names></name><xref ref-type="aff" rid="A21">u</xref><xref ref-type="aff" rid="A22">v</xref></contrib><contrib contrib-type="author"><name><surname>Krolak-Salmon</surname><given-names>Pierre</given-names></name><xref ref-type="aff" rid="A23">w</xref></contrib><contrib contrib-type="author"><name><surname>Lehmann</surname><given-names>Manja</given-names></name><xref ref-type="aff" rid="A1">a</xref><xref ref-type="aff" rid="A2">b</xref></contrib><contrib contrib-type="author"><name><surname>Magnin</surname><given-names>Eloi</given-names></name><xref ref-type="aff" rid="A24">x</xref></contrib><contrib contrib-type="author"><name><surname>Mendez</surname><given-names>Mario F.</given-names></name><xref ref-type="aff" rid="A25">y</xref></contrib><contrib contrib-type="author"><name><surname>Nestor</surname><given-names>Peter J.</given-names></name><xref ref-type="aff" rid="A26">z</xref></contrib><contrib contrib-type="author"><name><surname>Onyike</surname><given-names>Chiadi U.</given-names></name><xref ref-type="aff" rid="A27">aa</xref></contrib><contrib contrib-type="author"><name><surname>Pelak</surname><given-names>Victoria S.</given-names></name><xref ref-type="aff" rid="A28">bb</xref><xref ref-type="aff" rid="A29">cc</xref></contrib><contrib contrib-type="author"><name><surname>Pijnenburg</surname><given-names>Yolande</given-names></name><xref ref-type="aff" rid="A6">f</xref><xref ref-type="aff" rid="A7">g</xref></contrib><cont